{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "labels": ["dependencies"],
  "extends": ["config:base", "schedule:weekly", "group:allNonMajor"],
  "prHourlyLimit": 3,
  "ignorePaths": ["**/__tests__/**"],
  "rangeStrategy": "bump",
  "packageRules": [
    {
      "depTypeList": ["peerDependencies"],
      "enabled": false
    }
  ]
}
